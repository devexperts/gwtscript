{
  "name": "gwtscript",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "dependencies": {
    "fp-ts": "^2.9.5",
    "fs-extra": "^9.1.0",
    "typescript": "^4.2.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1"
  },
  "scripts": {
    "start:dev": "nodemon run.ts",
    "prettier:fix": "prettier --write ./src",
    "eslint": "eslint ./src/",
    "staged": "lint-staged",
    "start": "ts-node run.ts",
    "build": "tsc --project ./tsconfig.build.json",
    "typecheck": "tsc",
    "prepublish": "yarn jest && yarn build",
    "jest": "jest --no-cache",
    "jest:watch": "jest --watch"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "watch": [
      "./src",
      "./dir",
      "run.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --max-warnings=0"
    ]
  },
  "publishConfig": {
    "registry": "https://nexus.in.devexperts.com/repository/npm-dx-private/"
  },
  "jest": {
    "timers": "fake",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "preset": "ts-jest",
    "testURL": "http://localhost/",
    "testRegex": "/__tests__/.*\\.spec.ts$",
    "automock": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "lib"
    ]
  },
  "author": {
    "email": "enikiforov@devexperts.com",
    "name": "Eugine"
  }
}
